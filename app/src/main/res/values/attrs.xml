<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NestedScrollableHost">
        <attr name="scrollDirection" format="enum">
            <enum name="both" value="0" />
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
        <attr name="preferHorizontal" format="integer" />
        <attr name="preferVertical" format="integer" />
    </declare-styleable>

    <declare-styleable name="PlayPauseProgressButton">
        <attr name="circleCheckedFillColor" format="color" />
        <attr name="circleFillColor" format="color" />
        <attr name="circleFillDrawFullCircle" format="boolean" />
        <attr name="circleRingColor" format="color"></attr>
        <attr name="circleRingWidth" format="dimension"></attr>
        <attr name="circleShadowAlpha" format="integer"></attr>
        <attr name="circleShadowRadius" format="float"></attr>
        <attr name="circleShadowY" format="dimension"></attr>
        <attr name="clickable" format="boolean"></attr>
        <attr name="currentProgressColor" format="color"></attr>
        <attr name="focusable" format="boolean"></attr>
        <attr name="iconSize" format="dimension"></attr>
        <attr name="innerSize" format="dimension"></attr>
        <attr name="pauseBarDistance" format="dimension"></attr>
        <attr name="pauseBarHeight" format="dimension"></attr>
        <attr name="pauseBarWidth" format="dimension"></attr>
        <attr name="pauseIconSrc" format="integer"></attr>
        <attr name="pauseThrobColor" format="color"></attr>
        <attr name="playIconSrc" format="integer"></attr>
        <attr name="playPauseIconColor" format="color"></attr>
        <attr name="progressColorPaused" format="color"></attr>
        <attr name="pulseRingColor" format="color"></attr>
        <attr name="selectorColor" format="color"></attr>
        <attr name="showCircleShadow" format="boolean"></attr>
        <attr name="showInnerBorder" format="boolean"></attr>
        <attr name="showProgressOnRing" format="boolean"></attr>
        <attr name="showPulsingAnimation" format="boolean"></attr>
        <attr name="throbColor" format="color"></attr>
    </declare-styleable>

    <attr name="themedFullscreenPlayerPlayPauseCurrentProgressColor" format="color"></attr>
    <attr name="themedFullscreenPlayerPlayPausePulseRingColor" format="color"></attr>
    <attr name="themedFullscreenPlayerPlayPauseThrobColor" format="color"></attr>
    <attr name="themedIcDoneTintColorPrimaryDrawable" format="reference"></attr>
    <attr name="themedLinksDialogIcons" format="color"></attr>
    <attr name="themedMoreInfoShadow" format="reference"></attr>
    <attr name="themedPlayPauseProgressButtonSelectorColor" format="color"></attr>
    <attr name="themedPlayProgressColor" format="color"></attr>
    <attr name="themedPlayRingColor" format="color"></attr>
    <attr name="themedPlayedTextColor" format="color"></attr>
    <attr name="themedPlayingTextColor" format="color"></attr>
</resources>